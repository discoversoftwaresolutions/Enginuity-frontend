# Backend API URL (FastAPI)
REACT_APP_API_BASE_URL="https://enginuity-production.up.railway.app"

# Google Cloud Storage Bucket Name
REACT_APP_GCS_BUCKET="your-gcs-bucket-name"

# PostgreSQL Database (if frontend needs direct DB access)
REACT_APP_DB_URL="postgresql://your-user:your-password@your-db-host:5432/your-db-name"

# Redis for caching
REACT_APP_REDIS_URL="redis://your-redis-host:6379"

# Stripe API Keys (for payments)
REACT_APP_STRIPE_PUBLIC_KEY="your-stripe-public-key"

# Debug Mode
REACT_APP_DEBUG="true"
